{
  "name": "openkore-lsp-client",
  "description": "OpenKore Language Server Client",
  "author": "justJay-dev",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "justJay-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/justJay-dev/openkore-lsp"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "dependencies": {
    "@types/node": "^24.10.1",
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0"
  },
  "scripts": {
    "compile": "tsc -p ./"
  },
  "contributes": {
    "languages": [
      {
        "id": "openkore-config",
        "aliases": [
          "OpenKore Config",
          "openkore-config"
        ],
        "filenames": [
          "config.txt"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "openkore-items-control",
        "aliases": [
          "OpenKore Items Control",
          "openkore-items-control"
        ],
        "filenames": [
          "items_control.txt"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "openkore-mon-control",
        "aliases": [
          "OpenKore Monster Control",
          "openkore-mon-control"
        ],
        "filenames": [
          "mon_control.txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "openkore-config",
        "scopeName": "source.openkore-config",
        "path": "./syntaxes/openkore-config.tmLanguage.json"
      },
      {
        "language": "openkore-items-control",
        "scopeName": "source.openkore-items-control",
        "path": "./syntaxes/openkore-items-control.tmLanguage.json"
      },
      {
        "language": "openkore-mon-control",
        "scopeName": "source.openkore-mon-control",
        "path": "./syntaxes/openkore-mon-control.tmLanguage.json"
      }
    ]
  }
}